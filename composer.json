{
    "name": "tangoman/entity-helper",
    "description": "Symfony Entity Helper",
    "version": "2.1.3",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "TangoMan",
        "Symfony",
        "Entity",
        "Helper",
        "Traits"
    ],
    "authors": [
        {
            "name": "Matthias Morin",
            "email": "matthias.morin@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "require": {
        "ext-json":             "*",
        "ext-simplexml":        "*",
        "php":                  ">=5.5.0"
    },
    "require-dev": {
        "symfony/symfony":        "^3.4 !=3.4.13",
        "symfony/phpunit-bridge": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "TangoMan\\EntityHelper\\": ""
        }
    },
    "autoload-dev": {
        "classmap": [
            "Tests"
        ]
    },
    "extra": {
        "symfony-tests-dir": "Tests"
     },
    "minimum-stability": "dev",
    "prefer-stable": true
}
